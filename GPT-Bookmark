javascript:(function() {  var apiKey = prompt("Please enter your OpenAI API key:");  if (apiKey === null) {    return;  }  var text = prompt("Question for ChatGPT", "");  if (text !== null && text !== "") {    var model = "text-davinci-003";    fetch("https://api.openai.com/v1/engines/" + model + "/completions", {      method: "POST",      headers: {        "Content-Type": "application/json",        "Authorization": "Bearer " + apiKey      },      body: JSON.stringify({        prompt: text,        max_tokens: 150,        temperature: 0.5      })    })    .then(response => response.json())    .then(data => alert(data.choices[0].text))    .catch(error => console.error(error));  }})();
